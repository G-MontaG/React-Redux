{
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-one-expression-per-line": false
  },  
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  }
}
